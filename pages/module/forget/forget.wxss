.f1,.f3,.f4{
   /* border: 1px solid gold;   */
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-top: 1px solid #f0eded;
}
.f1 text,.f3 text,.f4 text{
  font-size: 28rpx;
  width: 25%;
  display: inline-block;
  height: 90rpx;
  line-height: 90rpx;
}
.f1 input,.f3 input,.f4 input{
  width: 75%;
  float: right;
  font-size: 28rpx;
  height: 90rpx;
  line-height: 90rpx;
}
.f2{
  width: 100%;
  height: 90rpx;
  position: relative;
   border-top: 1px solid #f0eded;
   /* border: 1px solid gold;   */
}
.f2 text:first-child{
  font-size: 28rpx;
  width: 25%;
  display: inline-block;
  height: 90rpx;
  line-height: 90rpx;
  position: absolute;
  left: 0;
}
.t2{
  font-size: 28rpx;
  width: 30%;
  display: inline-block;
  height: 90rpx;
  line-height: 90rpx;
  position: absolute;
  right: 0;  
  border-left: 1px solid #f0eded;
}
.f2 input{
 font-size: 28rpx;
  position: absolute;
  left: 25%;
  height: 90rpx;
  line-height: 90rpx;
}
.btn1{
  margin-top: 30rpx;
  width: 25%;
  font-size: 28rpx;
}
.f4{
  border-bottom: 1px solid #f0eded;
}