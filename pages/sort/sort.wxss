.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}

.log-item {
  margin: 10rpx;
}

.meauleft {
  width: 31%;
  height: 100%;
  display: inline-block;
  margin-top: 130rpx;
  position: fixed;
  font-size: 24rpx;
  background: #f8f8f8;
  left: 0;
}
.Search{
  
}
.meauleft view, .meauleft text {
  text-align: left;
  display: inline-block;
  height: 40px;
  line-height: 40px;
  width: 100%;
}

.meaurig {
  width: 66%;
  height: 1500px;
  float: right;
  margin-top: 140rpx;
}

.meau1 {
  display: inline-block;
  width: 100rpx;
  height: 30rpx;
}

.meau1 img {
  width: 100%;
  height: 50%;
}

.meaurig_ul {
  display: flex;
  width: 100%;
  justify-content: flex-start;
  flex-wrap: wrap;
  flex-direction: row;
}

.meaurig_ul li {
  display: inline-block;
  width: 120rpx;
  height: 60rpx;
  background: red;
  position: relative;
  margin-top: 20rpx;
  margin-left: 17rpx;
}

.meaurig_ul li image {
  width: 100%;
  height: 100%;
  position: absolute;
}

.meau2 {
  display: inline-block;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  font-weight: 600;
  margin-left: 17rpx;
  overflow: hidden;
}

.meau2_ul {
  display: flex;
  width: 100%;
  justify-content: space-between;
  flex-wrap: wrap;
  flex-direction: row;
  margin-top: 20rpx;
}

.meau3 {
  margin-top: 20rpx;
  width: 45%;
  border: 1px solid #f5f5f5;
  text-align: center;
  margin-left: 17rpx;
  font-size: 24rpx;
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  background: #f5f5f5;
}

.recommend {
  margin-top: 20rpx;
  font-size: 28rpx;
  font-weight: 600;
  margin-left: 17rpx;
  overflow: hidden;
}

.meauleft view {
  text-indent: 40rpx;
}
.cur{
  background: #ffffff;
}
.hh{
  display: block;
}
.nobg{
  background: none;
}